<p id="notice"><%= notice %></p>
<div class="row">
<div class="col-md-10"> <h3 style="margin-top:3px">Downloads</h3> </div>
<div class="col-md-2"> <%= button_to 'Back', root_path, method: "get" %> </div>
</div>

<table class="new_table table">
  <thead>
    <tr>
      <!--<th>Title</th>
      <th>Accesslevel</th>-->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @downloads.each do |download| %>
      <tr>
        <td><%= download.title %></td>
        <!--<td><%= download.accesslevel %></td> -->
        <td><%= link_to 'Show', download %></td>
        <td><%= link_to 'Edit', edit_download_path(download) %></td>
        <td><%= link_to 'Delete', download, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= button_to 'New Download', new_download_path, method: "get" %>
