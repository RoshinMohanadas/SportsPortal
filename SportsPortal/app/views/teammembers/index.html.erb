<p id="notice"><%= notice %></p>

<h1>Teammembers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Dob</th>
      <th>Email</th>
      <th>Gender</th>
      <th>Role</th>
      <th>Achievements</th>
      <th>Foodpreference</th>
      <th>Contactno</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teammembers.each do |teammember| %>
      <tr>
        <td><%= teammember.name %></td>
        <td><%= teammember.dob %></td>
        <td><%= teammember.email %></td>
        <td><%= teammember.gender %></td>
        <td><%= teammember.role %></td>
        <td><%= teammember.achievements %></td>
        <td><%= teammember.foodpreference %></td>
        <td><%= teammember.contactno %></td>
        <td><%= teammember.team_id %></td>
        <td><%= link_to 'Show', teammember %></td>
        <td><%= link_to 'Edit', edit_teammember_path(teammember) %></td>
        <td><%= link_to 'Destroy', teammember, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Teammember', new_teammember_path %>
