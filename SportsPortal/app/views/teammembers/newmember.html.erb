
<h1> Team members</h1>

<%= link_to 'Submit Team', {:controller => :teams, :action => :createteam, :teamid => @team.id, :eventid => @team.event_id}, method: "post" %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Dob</th>
      <th>Email</th>
      <th>Gender</th>
      <th>Role</th>
      <th>Achievements</th>
      <th>Foodpreference</th>
      <th>Contactno</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.dob %></td>
        <td><%= member.email %></td>
        <td><%= member.gender %></td>
        <td><%= member.role %></td>
        <td><%= member.achievements %></td>
        <td><%= member.foodpreference %></td>
        <td><%= member.contactno %></td>
        <td><%= member.team_id %></td>
         <td><%= link_to 'Show'%></td>
        <td><%= link_to 'Edit'%></td>
        <td><%= link_to 'Destroy' %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= render 'formmember', teammember: @member%>